{
    "name": "evence/soft-deleteable-extension-bundle",
    "description": "Fixed to work with Symfony 5.0+. Adjusted from E-vence/SoftDeleteableListenerExtensionBundle.",
    "authors": [{
        "name": "Ruben Harms",
        "homepage": "http://www.rubenharms.nl/"
    }],
    "keywords": ["soft-deleteable"],
    "require": {
        "php": "^7.1.3",
        "symfony/framework-bundle": "^4.2|^5.0"
    },
    "require-dev": {
        "symfony/console": "^4.2|^5.0"
    },
    "target-dir": "Evence/Bundle/SoftDeleteableExtensionBundle",
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    },
    "autoload": {
        "psr-0": {
            "Evence\\Bundle\\SoftDeleteableExtensionBundle": ""
        }
    },
    "type": "symfony-bundle"
}
